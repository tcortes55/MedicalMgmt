            @Html.DisplayFor(modelItem => item.User.Username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Expertise)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GraduationUni)
        </td>
